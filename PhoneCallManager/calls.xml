<!-- example layout of calls.xml -->
<calls>
    <!-- 0...n calls -->
    <!-- unique cid - used as key -->
    <call cid="n">
        <length></length> <!-- non empty int > 0 (in seconds) -->
     <!--   <price>\</price> non empty float, 2 decimals precision, > 0 -> calculated from config.xml -->
        <callee></callee> <!-- non empty string, length 9 -->
        <destination></destination> <!-- non empty, country id from config.xml? -->
        <direction></direction> <!-- one of "in" and "out" case-insensitive -->
        <dateTime>YYYY-MM-DDThh:mm:ss</dateTime> <!-- non empty, xs:dateTime -->
    </call>
    
</calls>
